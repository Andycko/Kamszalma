{% extends "_layout" %}

{% set title = "404" %}
{% set subTitle = "Page Not Found" %}

{% block content %}
    <div class="no_header text-center">
        <div class="container">
            <h1 class="pb-5">{{ subTitle }}</h1>
            <p>The page you’re looking for may no longer exist or the URL may be incorrect. Apologies for the
                inconvenience.</p>
            <p>Please try again from the <a href="/" class="text-white"><b>homepage</b> →</a></p>
        </div>
    </div>
{% endblock %}
